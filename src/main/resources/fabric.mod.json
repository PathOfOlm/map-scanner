{
  "schemaVersion": 1,
  "id": "map-frame-addon",
  "version": "${version}",
  "name": "Map Frame Addon",
  "description": "Automatically finds map item frames, stores maps, and logs to Discord.",
  "authors": [
    "PathOfOlm"
  ],
  "contact": {
    "sources": "https://github.com/MeteorDevelopment/meteor-addon-template"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.frames.MapFrameAddon"
    ]
  },
  "custom": {
    "meteor-client:color": "225,25,25",
    "modmenu": {
      "parent": {
        "id": "meteor-client"
      }
    }
  },
  "depends": {
    "java": ">=21",
    "minecraft": "${mc_version}",
    "meteor-client": "*"
  }
}
